{
  "name": "marcusrbrown",
  "version": "0.0.0",
  "description": "My GitHub public profile README and tool configurations.",
  "repository": "https://github.com/marcusrbrown/marcusrbrown",
  "license": "MIT",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline --loglevel warn",
    "fix": "pnpm run fix:markdown && pnpm run fix:eslint",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lint": "markdownlint-cli2 && eslint --flag unstable_config_lookup_from_file .",
    "fix:eslint": "eslint --fix --flag unstable_config_lookup_from_file .",
    "fix:markdown": "markdownlint-cli2 --fix",
    "test": "exit 0"
  },
  "prettier": "@bfra.me/prettier-config/120-proof",
  "devDependencies": {
    "@bfra.me/eslint-config": "0.25.2",
    "@bfra.me/prettier-config": "^0.16.0",
    "@bfra.me/tsconfig": "^0.11.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jiti": "2.1.2",
    "markdownlint": "0.38.0",
    "markdownlint-cli2": "0.18.1",
    "prettier": "3.6.2"
  },
  "packageManager": "pnpm@10.14.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
